<div id="body">
    <form method="get">
        <h3>Rellena los campos requeridos</h3>
       <div class="row">
           <div class="col-md-4">
               <strong>
                   <font color="Teal">
                       Username:
                   </font>
               </strong>
               <br />
               <input type="text" placeholder="Username" autofocus="" name="user" value="" size="20" required="" maxlength="30" /><br />
               <br />
           </div>

           <div class="col-md-4">
               <strong>
                   <font color="Teal">
                       Password:
                   </font>
               </strong><br />
               <input type="password" placeholder="Password" required="" autofocus="" name="pass" size="10" maxlength="9" /><br />
               <br />
           </div>

           <div class="col-md-4">
               <strong>
                   <font color="Teal">
                       Name:
                   </font>
               </strong><br />
               <input type="text" name="nombre" placeholder="Name" value="" autofocus="" size="20" required="" maxlength="30" /><br />
               <br />
           </div>
        </div> 
<div class="row">
    <div class="col-md-4">
        <strong>
            <font color="Teal">
                LastName:
            </font>
        </strong><br />
        <input type="text" placeholder="LastName" name="apellidos" value="" autofocus="" size="20" required="" maxlength="30" /><br />
        <br />
    </div>

    <div class="col-md-4">
        <strong>
            <font color="Teal">
                Direction:
            </font>
        </strong><br /> <input type="text" placeholder="Direction" autofocus="" name="direccion" value="" size="20" required="" maxlength="30" /><br />
        <br />
    </div>

    <div class="col-md-4">
        <strong>
            <font color="Teal">
                E-mail:
            </font>
        </strong><br /> <input name="correo" id="inputEmail" autofocus="" placeholder="Email address" required="" type="email" value="" size="20" maxlength="30" /><br />
        <br />
    </div>
</div>      
        <div class="row">
            <div class="col-md-4">
                <strong>
                    <font color="Teal">
                        Phone:
                    </font>
                </strong><br />
                <input type="text" placeholder="Phone" autofocus="" name="telefono" size="10" required="" maxlength="9" /><br />
                <br />
            </div>
        </div>

            <strong>
                <font color="Teal">
                    Sexo
                </font>
            </strong>
            <br />

    <input type="radio" name="sexo" value="hombre" checked="checked" />
        <strong> 
            <font color="Teal">
                Hombre
            </font>
            <br />
        </strong> 

            <input type="radio" name="sexo" value="mujer" />
        <strong> 
            <font color="Teal">
                Mujer
            </font>
        </strong>
        <br /><br />

            <font color="Teal">
                Incluir mi foto
            </font> 
        <input type="file" name="foto" />
            <br />
            <input name="suscribir" type="checkbox" value="suscribir" />
    
        <font color="Teal">
            Suscribirme al boletín de novedades
        </font>
        <br /><br />

        <input type="submit" name="enviar" class="btn btn-success" value="Guardar cambios" />

        <input type="reset" name="limpiar" class="btn btn-danger" value="Borrar los datos introducidos" />
        <br /> <br />
        <div class="row">
            </div>
            <div class="col-md-4">
                <h3 style="color: #fff; margin-bottom: 10px; margin-left: 5px;">Contacto</h3>

                    <div class="form-group">
                        <input class="form-control" id="email" name="email" placeholder="Correo" required="required" type="email">
                    </div>
                    <div class="form-group">
                        <textarea class="form-control" id="mensaje" name="message" placeholder="¿En que te podemos ayudar?" rows="6" required="required"></textarea>
                    </div>
                    <button type="submit" class="btn btn-primary btn-sm" style="width: 70%; margin-left: 15%; ">Enviar</button>
                </div>
            <div class="col-md-2"></div>
        </div>

        <div class="row">
            <div class="col-md-11"></div>
            <div class="col-md-1">
                <label>
                    <a href="#Top">Top</a>
                </label>
            </div>
        </div>
       
    </form>
</div>
